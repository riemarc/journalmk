{
    "notes_directory_names": ["_notes", "__Notes"],
    "notes_pdf_export_commands": {
        "xopp": "xournalpp {xopp} -p {pdf}",
        "ipynb": "jupyter nbconvert {ipynb} --to pdf --output {pdf}",
        "pdfnote": "cp {pdfnote} {pdf}",
        "jpg": "convert {jpg} {pdf}"
    },
    "notes_pdf_inplace_export_commands": {
        "odt": "libreoffice --convert-to pdf {odt} --outdir {outdir}"
    },
    "journal_type": "topological",
    "datetime_filename_formats": ["%Y-%m-%d-Note-%H-%M", "%Y_%m_%d %H_%M Office Lens"],
    "exclude_directories": [
        ["/", "home", "marcus", "Dokumente", "Projekte", "journalmk", "journalmk", "example"]
    ],
    "exclude_note_endings": ["autosave.xopp"],
    "journal_period": [425600]
}
